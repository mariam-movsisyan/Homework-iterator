{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "no-console": false,
        "no-unused-variable": true,
        "interface-name": true,
        "variable-name": {
            "options": [
                "allow-leading-underscore"            
            ]
        }
    },
    "rulesDirectory": []
}